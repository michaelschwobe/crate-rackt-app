{
  "name": "crate-rackt-app",
  "description": "An opinionated create-react-app template/starting point",
  "version": "0.5.0",
  "license": "MIT",
  "private": true,
  "author": "Michael Schwobe <mschwobe@gmail.com> (http://michaelschwobe.com)",
  "repository": "github:michaelschwobe/crate-rackt-app",
  "scripts": {
    "clean": "rimraf .eslintcache .stylelintcache build coverage storybook-static",
    "lint": "npm-run-all --sequential lint:js lint:css",
    "lint:js": "eslint .",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:fix": "npm-run-all --sequential 'lint:js --fix' 'lint:css --fix'",
    "test": "react-scripts test",
    "test:coverage": "npm-run-all --sequential 'test --bail --coverage --watchAll=false'",
    "test:changed": "npm-run-all --sequential 'test:coverage --changedSince=origin/master'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.1",
    "classnames": "^2.3.0",
    "history": "^5.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.0-beta.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.2.1",
    "@storybook/addon-essentials": "^6.2.1",
    "@storybook/addon-links": "^6.2.1",
    "@storybook/node-logger": "^6.2.1",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.1",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "source-map-explorer": "^2.5.2",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^21.0.0"
  },
  "resolutions": {
    "@storybook/react/babel-loader": "8.1.0"
  },
  "engines": {
    "node": ">=14",
    "yarn": "^1.22.10"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
